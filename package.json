{
  "name": "ai-logistics-optimization",
  "version": "1.0.0",
  "description": "AI-Powered Logistics Optimization Platform with Frontend and Backend",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "sync-ports": "node sync-ports.js",
    "predev": "./kill-ports.sh && npm run sync-ports",
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"blue,magenta\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && PORT=3003 node src/app.js",
    "dev:frontend": "cd frontend && NEXT_PUBLIC_API_URL=http://localhost:3003 NEXT_PUBLIC_WS_URL=ws://localhost:8081 npm run dev -- -p 3001",
    "dev:all": "npm run dev",
    "build": "concurrently \"npm run build:backend\" \"npm run build:frontend\"",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "npm run dev",
    "start:backend": "cd backend && npm run start",
    "start:frontend": "cd frontend && npm run start",
    "stop": "pkill -f 'node src/app.js' ; pkill -f 'next dev' ; echo 'All services stopped'",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm install && npm run install:backend && npm run install:frontend",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules backend/.next frontend/.next",
    "health": "curl -s http://localhost:3003/health/detailed | json_pp",
    "logs": "concurrently \"npm run logs:backend\" \"npm run logs:frontend\"",
    "logs:backend": "cd backend && tail -f logs/*.log",
    "logs:frontend": "cd frontend && tail -f logs/*.log",
    "dev:websocket": "cd backend && WS_PORT=8081 node src/demo/websocket-server.js"
  },
  "keywords": [
    "logistics",
    "optimization",
    "AI",
    "route planning",
    "delivery",
    "monorepo"
  ],
  "author": "Abdullah Alkurdi <89.kurdi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "express-rate-limit": "^8.2.1",
    "pg": "^8.16.3"
  },
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.8.2"
  }
}